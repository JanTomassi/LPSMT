<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_library">

    <include-dynamic
        android:id="@+id/navigation_library"
        android:label="@string/title_library"
        app:graphPackage="${applicationId}.library"
        app:graphResName="library_nav"
        app:moduleName="library" />

    <include-dynamic
        android:id="@+id/navigation_tracker"
        android:label="@string/title_tracker"
        app:graphPackage="${applicationId}.tracker"
        app:graphResName="tracker_nav"
        app:moduleName="tracker"/>
</navigation>